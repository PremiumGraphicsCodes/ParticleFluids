<UserControl x:Class="PG.CGStudio.Selection.PolygonMeshObjectSelectView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PG.CGStudio.Selection"
             xmlns:shape="clr-namespace:PG.Control.Shape;assembly=PGControl"
             mc:Ignorable="d">
    <StackPanel>
        <Button Content="Pick" Command="{Binding PickCommand}"/>
        <shape:PolygonMeshView/>
    </StackPanel>
</UserControl>
